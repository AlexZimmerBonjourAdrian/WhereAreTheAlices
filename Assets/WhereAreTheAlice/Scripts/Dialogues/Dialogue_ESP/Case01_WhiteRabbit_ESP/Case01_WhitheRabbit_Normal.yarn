<<set $habitacion to 1>>
<<set $vida to 10>>
<<set $llave to false>>
<<set $pocion to false>>
<<set $espada to false>>
<<set $enemigo_derrotado to false>>
<<set $juego_terminado to false>>
<<set $enemigo_vida to 0>>
<<set $enemigo_daño to 0>>
<<set $jugador_daño to 1>>

// Game Loop
<<if $juego_terminado is false>>
  // Display Current Room
  <<jump mostrar_habitacion>>
<<else>>
  <<jump game_over>>
<<endif>>

