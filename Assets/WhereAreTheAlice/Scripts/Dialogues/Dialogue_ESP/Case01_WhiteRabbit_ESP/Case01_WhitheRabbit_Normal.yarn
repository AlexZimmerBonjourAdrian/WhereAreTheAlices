    <<jump mostrar_habitacion>>
  <<else>>
    El monstruo te ataca. Te hace {$enemigo_daño} de daño.
    <<set $vida to $vida - $enemigo_daño>>
    <<if $vida <= 0>>
        <<set $juego_terminado to true>>
        <<jump game_over>>
    <<else>>
       <<jump batalla>>
    <<endif>>
  <<endif>>
->[Salir]
    <<set $juego_terminado to true>>
    <<jump game_over>>

